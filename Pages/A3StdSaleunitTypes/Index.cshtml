@page
@model ControlCenterApp.Pages.A3StdSaleunitTypes.IndexModel
@{
    ViewData["Title"] = "Saleunit Types";
}

<h1>Saleunit Types</h1>

<p>
    <a asp-page="Create" class="btn btn-primary">Create New</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>Type</th>
            <th>Description</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Items)
{
        <tr>
            <td>@item.SaleunitType</td>
            <td>@item.ObjDescription</td>
            <td>
                <a asp-page="Edit" asp-route-id="@item.ObjId">Edit</a> |
                <a asp-page="Delete" asp-route-id="@item.ObjId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
